{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"semicolon": [
			true,
			"never"
		],
		"quotemark": [
			true,
			"single",
			"jsx-double",
			"avoid-escape"
		],
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"interface-name": [
			true,
			"never-prefix"
		],
		"max-line-length": [
			true,
			160
		],
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"no-console": [
			false,
			"log"
		],
    "no-empty-interface": false,
    "no-unused-expression": false,
    "only-arrow-functions": false,
		"interface-over-type-literal": false,
		"prefer-for-of": false
	},
	"rulesDirectory": []
}
